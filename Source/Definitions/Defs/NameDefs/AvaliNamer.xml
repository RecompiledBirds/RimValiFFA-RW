<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <RulePackDef>
        <defName>RVFFA_AvaliNamer</defName>
        <rulePack>
            <rulesStrings>
                <li>Name->[FirstPartName][OtherPartName] [LastNameFirstPart][LastNameLastPart]</li>
				<li>Name->[FirstPartName][OtherPartName] '[NickName]' [LastNameFirstPart][LastNameLastPart]</li>
            </rulesStrings>
            <rulesRaw>
                <li Class="Rule_File">
                    <keyword>FirstPartName</keyword>
                    <path>RVFFANameGen/FirstSectionName</path>
                </li>
                <li Class="Rule_File">
                    <keyword>OtherPartName</keyword>
                    <path>RVFFANameGen/LastSectionName</path>
                </li>
				<li Class="Rule_File">
					<keyword>LastNameFirstPart</keyword>
					<path>RVFFANameGen/LastNameFirstSection</path>
				</li>
				<li Class="Rule_File">
					<keyword>LastNameLastPart</keyword>
					<path>RVFFANameGen/LastNameLastSection</path>
				</li>
				<li Class="Rule_File">
					<keyword>NickName</keyword>
					<path>RVFFANameGen/NickNames</path>
				</li>
            </rulesRaw>
        </rulePack>
        
    </RulePackDef>
</Defs>