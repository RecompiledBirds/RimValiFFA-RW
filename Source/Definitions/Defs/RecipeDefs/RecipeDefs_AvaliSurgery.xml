<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef ParentName="RVFFA_SurgeryInstallBodyPartNaturalBase">
		<defName>RVFFA_InstallNaturalAvaliTail</defName>
		<label>install tail</label>
		<description>Install a biological tail.</description>
		<descriptionHyperlinks>
			<ThingDef>RVFFA_Tail</ThingDef>
		</descriptionHyperlinks>
		<jobString>Installing tail.</jobString>
		<deathOnFailedSurgeryChance>0.0</deathOnFailedSurgeryChance>
		<skillRequirements>
			<Medicine>8</Medicine>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>RVFFA_Tail</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>RVFFA_Tail</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>RVFFA_Tail</li>
		</appliedOnFixedBodyParts>
	</RecipeDef>


	<RecipeDef ParentName="RVFFA_SurgeryInstallBodyPartNaturalBase">
		<defName>RVFFA_PreenWingFeathers</defName>
		<label>preen wing feathers</label>
		<description>Preen an avali's wing feathers.</description>
		<jobString>Preening wing feathers.</jobString>
		<deathOnFailedSurgeryChance>0.0</deathOnFailedSurgeryChance>
		<skillRequirements>
			<Medicine>0</Medicine>
		</skillRequirements>
		<appliedOnFixedBodyParts>
			<li>RimValiArmFeathers</li>
		</appliedOnFixedBodyParts>
	</RecipeDef>
</Defs>