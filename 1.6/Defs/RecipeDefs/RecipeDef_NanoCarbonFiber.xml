<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <RecipeDef Name="RVFFA_BaseNanoCRecipe" Abstract="True">
        <description>Weave nano-carbon fiber.</description>
        <jobString>Weaving nano-carbon fiber.</jobString>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <effectWorking>Smelt</effectWorking>
        <workAmount>100</workAmount>
        <soundWorking>Recipe_Smelt</soundWorking>
		<researchPrerequisites>
			<li>RVFFA_CarbonNanoFiberTech</li>
		</researchPrerequisites>
    </RecipeDef>

    <RecipeDef ParentName="RVFFA_BaseNanoCRecipe">
        <defName>RVFFA_NanoCarbonFiberRecipe</defName>
        <label>Weave nano-carbon fiber</label>
        <ingredients>
            <li>
                <filter>
                <thingDefs>
                    <li>RVFFA_NanoFiber</li>
                </thingDefs>
                </filter>
                <count>2</count>
            </li>
            <li>
                <filter>
                <thingDefs>
                    <li>RVFFA_RefinedCarbon</li>
                </thingDefs>
                </filter>
                <count>5</count>
            </li>
        </ingredients>
        <products>
            <RVFFA_NanoCarbonFiber>10</RVFFA_NanoCarbonFiber>
        </products>
    </RecipeDef>

